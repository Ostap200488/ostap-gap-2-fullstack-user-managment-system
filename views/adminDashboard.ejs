
<!DOCTYPE html>
<html>
<head>
    <title>Admin Dashboard</title>
</head>
<body>
    <h1>Admin Dashboard</h1>
    <h2>All Users</h2>
    <ul>
        <% users.forEach(user => { %>
            <li><%= user.name %> - <%= user.email %> (<%= user.role %>)</li>
        <% }) %>
    </ul>
    <a href="/logout">Logout</a>
</body>
</html>
